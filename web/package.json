    "lint": "eslint --max-warnings=0 src",
    "jest": "jest src",
    "cy:open": "cypress open",
    "cy:server": "npm run dev & wait-on 'http://localhost:9999'",
    "cy:firefox": "cypress run --browser firefox --headless",
    "cy:chrome": "cypress run --browser chrome --headless"
  },
  "repository": {
